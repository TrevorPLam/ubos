    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "tsx script/build.ts",
    "start": "cross-env NODE_ENV=production node dist/index.cjs",
    "check": "tsc -p tsconfig.json && tsc -p tsconfig.node.json",
    "lint": "eslint \"client/src/**/*.{ts,tsx,js,jsx}\" \"server/**/*.{ts,tsx,js,jsx}\" \"shared/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:backend": "vitest run --config vitest.config.ts",
    "test:frontend": "vitest run --config vitest.config.client.ts",
    "test:ci": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "validate:security": "npm run check && npm run test:backend && npm run test:frontend && npm run coverage && npm run build && npm run security:check-tests",
    "security:check-tests": "node -e \"const fs=require('fs');const glob=require('glob');const files=glob.sync('tests/**/*.test.ts');let hasOnly=false;files.forEach(f=>{const content=fs.readFileSync(f,'utf8');if(/\\.(only|skip)\\(/.test(content)){console.error('ERROR: Found .only() or .skip() in '+f);hasOnly=true;}});if(hasOnly)process.exit(1);console.log('âœ“ No focused/skipped tests found');\"",
    "generate-task-index": "tsx script/generate-task-index.ts",
    "postinstall": "patch-package"
  },
